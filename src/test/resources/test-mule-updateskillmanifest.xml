<?xml version="1.0" encoding="UTF-8"?>

<mule 
	xmlns:alexa="http://www.mulesoft.org/schema/mule/alexa"
	xmlns="http://www.mulesoft.org/schema/mule/core" xmlns:doc="http://www.mulesoft.org/schema/mule/documentation" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
http://www.mulesoft.org/schema/mule/alexa http://www.mulesoft.org/schema/mule/alexa/current/mule-alexa.xsd">
	<configuration-properties file="automation-credentials.properties" />
	<alexa:config name="Alexa_Config" doc:name="Alexa Config" doc:id="a1b20c0f-e314-4b93-aa65-f49df33f1fc4" >
		<alexa:token-connection token="${alexa.token}" />
	</alexa:config>
	
	
	<flow name="to-certifyFlow1" doc:id="f7772c8b-13ff-43d1-98ee-c1b7dba1b47e" >
		<alexa:update-skill-manifest doc:name="Update skill manifest" doc:id="7b586b41-8a2e-49fb-89a2-f0bb103fe464" config-ref="Alexa_Config" skillId="amzn1.ask.skill.11c286e9-e0fc-4d96-8e68-816a8a16126e">
			<alexa:manifest>
				<alexa:publishing-information testingInstructions="Say 'Alexa, hello world'" category="HEALTH_AND_FITNESS">
					<alexa:distribution-countries >
						<alexa:distribution-country value="US" />
					</alexa:distribution-countries>
					<alexa:locale skillName="updating skill name" summary="to updat eskill name" description="hjbwehbw" >
						<alexa:keywords >
							<alexa:keyword value="Descriptive_Phrase_1" />
							<alexa:keyword value="Descriptive_Phrase_2" />
						</alexa:keywords>
						<alexa:example-phrases >
							<alexa:example-phrase value="Alexa, open sample custom skill." />
						</alexa:example-phrases>
					</alexa:locale>
				</alexa:publishing-information>
				<alexa:privacy-and-compliance />
				<alexa:events >
					<alexa:endpoint uri="https://smallUri.com" sslCertificateType="Trusted" />
					<alexa:subscriptions >
						<alexa:sub-scription EventName="SKILL_ENABLED" />
					</alexa:subscriptions>
				</alexa:events>
				<alexa:apis >
					<alexa:custom >
						<alexa:endpoint uri="https://largeUri.com" sslCertificateType="Trusted" />
						<alexa:interfaces >
							<alexa:interface type="ALEXA_PRESENTATION_APL" />
						</alexa:interfaces>
					</alexa:custom>
				</alexa:apis>
				<alexa:permissions >
					<alexa:permission PermissionName="alexa::devices:all:address:full:read" />
				</alexa:permissions>
			</alexa:manifest>
		</alexa:update-skill-manifest>
	</flow>
</mule>
